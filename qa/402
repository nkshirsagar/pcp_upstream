seq=`basename $0`
echo "QA output created by $seq"

# get standard filters
. ./common.product
. ./common.filter
. ./common.check

status=1	# failure is the default!
trap "rm -f $tmp.*; exit \$status" 0 1 2 3 15

# real QA test starts here

pmprobe -a archives/540117-short.0  -v irix.pmda.uname hinv.ncpu

pminfo -a archives/540117-short.0  -f irix.pmda.uname hinv.ncpu

# success, all done
status=0
exit
