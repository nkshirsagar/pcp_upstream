pipe {
    numclients		PIPE:0:0
    maxmemory		PIPE:0:1
    commands		PIPE:0:2
    line		PIPE:0:3
    firehose		PIPE:0:4
    queue
}

pipe.queue {
    memused		PIPE:0:5
    count		PIPE:0:6
    bytes		PIPE:0:7
    numclients		PIPE:0:8
}
