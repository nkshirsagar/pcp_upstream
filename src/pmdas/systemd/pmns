systemd {
    numclients		SYSTEMD:0:0
    maxmem		SYSTEMD:0:1
    journal
}

systemd.journal {
    records             SYSTEMD:2:0
    records_raw         SYSTEMD:2:1
    field
    count               SYSTEMD:2:2
    bytes               SYSTEMD:2:3
}

systemd.journal.field {
    cursor              SYSTEMD:1:0
    string              SYSTEMD:1:1 /* possibly a string  */
    blob                SYSTEMD:1:2 /* a binary blob */
}
