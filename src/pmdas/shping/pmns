shping {
    status	SHPING:0:6
    error	SHPING:0:10
    time
    cmd		SHPING:0:7
    control
}

shping.time {
    real	SHPING:0:0
    cpu_usr	SHPING:0:1
    cpu_sys	SHPING:0:2
}

shping.control {
    numcmd	SHPING:0:3
    cycles	SHPING:0:9
    cycletime	SHPING:0:4
    timeout	SHPING:0:5
    debug	SHPING:0:8
}
